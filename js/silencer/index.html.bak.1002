                        <li><a href="/#/modules"> Model </a></li>
                        <li><a href="/#/inputs"> Data </a></li>
                    </ul>
                </li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li style="width: 100%">
                    <div class="navbar-form" style="width: 100%">
                        <div class="input-group has-feedback" style="width: 100%">
                            <button class="btn glyphicon glyphicon-search" onclick="processRequest()"
                                    style="z-index: 200;margin-left:-40px;border: none;outline: none;background: transparent;color: #e6e6e6"></button>
                            <input id="search-text" type="text" placeholder="Tx Hash, Address, or Block #"
                                   required autocomplete="off" class="form-control searchi" style="border-radius: 3px">
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</nav>

<div class="main-body">
    <div id="app"></div>
</div>

